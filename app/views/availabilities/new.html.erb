<%= form_with model: @availability, local: true do |form| %>
  <div class="row">
    <div class="form-group col-sm-6">
      <%= form.label :from, class: "form-control-label" %>
    </div>
    <div class="form-group col-sm-6">
      <%= form.label :to,class: "form-control-label" %>
    </div>
  </div>
  <div class="bottom-btn btn-row">
    <%= form.button(type: 'submit', class: "btn btn-primary btn-sm") do %>
      <i class="fa fa-check left" aria-hidden="true"></i>Save
    <% end %>
  </div>
<% end %>
